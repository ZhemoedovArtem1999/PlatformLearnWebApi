{
  "ReverseProxy": {
    "Routes": {
      "swagger-route": {
        "ClusterId": "api-cluster",
        "Match": {
          "Path": "/swagger/{**catch-all}"
        }
      },
      "api-route": {
        "ClusterId": "api-cluster",
        "Match": {
          "Path": "/api/{**catch-all}"
        }
      },
      "grpc-route": {
        "ClusterId": "grpc-cluster",
        "Match": {
          "Path": "/auth/{**catch-all}",
          "Headers": [
            {
              "Name": "Content-Type",
              "Values": [
                "application/grpc",
                "application/grpc-web-text",
                "application/grpc-web"
              ]
            }
          ]
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/auth" // Удаляем префикс перед передачей на сервер
          }
        ]
      }
    },
    "Clusters": {
      "api-cluster": {
        "Destinations": {
          "api-server": {
            "Address": "http://localhost:7243/"
          }
        }
      },
      "grpc-cluster": {
        "Destinations": {
          "grpc-server": {
            "Address": "http://localhost:5777/"
          }
        },
        "HttpRequest": {
          "VersionPolicy": "RequestVersionExact",
          "Version": "2"
        }
      }
    }
  }
}